{
  "blob_auth": {
    "type": "object",
    "value": {
      "type": "Raw",
      "scheme": "Key",
      "parameter": "@appsetting('azureblob-connectionKey')"
    }
  },
  "sb_auth": {
    "type": "object",
    "value": {
      "type": "Raw",
      "scheme": "Key",
      "parameter": "@appsetting('servicebus-connectionKey')"
    }
  },
  "api_scope": {
    "type": "string",
    "value": "@appsetting('api_scope')"
  },
  "apim_sub_key": {
      "type": "string",
      "value": "@appsetting('apim_sub_key')"            
  },

  "client_id": {
      "type": "string",
      "value": "@appsetting('client_id')"
  },
  "client_secret": {
      "type": "string",
      "value": "@appsetting('client_secret')"
  },
  "tenant_id": {
      "type": "string",
      "value": "@appsetting('tenant_id')"
  }
}
